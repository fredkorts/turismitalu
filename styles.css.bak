body
{
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	background-image: url('tagataust.jpg');
	background-size: cover !important;
	background-attachment: fixed;
	height: 100%;
	width: 100%;
}

#front_page
{
	width: 100%;
	height: 100%;
}



/*FLAGS STARTS*/
#flags
{
	width: 100%;
	height: 3%;
}

#flags ul
{
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 100%;
	height: 100%;
}

#flags ul li
{
	display: inline;
	margin-left: 10px;
}

#flags img
{
	height: 100%;
}
/*FLAGS ENDS*/




/*NAVIGATION STARTS*/

#navigation
{
	width: 100%;
	height: 5%;
}

#navigation ul
{
	list-style-type: none;
	margin: 0 auto;
	padding: 0;
	text-align: center;
	width: 100%;
	height: 100%;
}

#navigation ul li
{
	display: inline-block;
	margin-left: 10px;
	color: white;
	margin-top: 10px;
	margin-left:35px;
}

#navigation a
{
	font-size: 28px;
	font-family: 'Droid Sans';
	font-weight: bold;
	text-decoration: none;
	color: black;
}
/*NAVIGATION ENDS*/



#about
{
	height: 83%;
	width: 50%;

}

#top_fence
{
	width: 100%;
	height: 20%;


}

#about_circle
{
	width: 860px;
	height:550px;
	background: url('tagataust2.png') no-repeat;
	border-radius: 100%;
	background-size: 100%;
	margin: 0 auto;
	text-align: center;
}
#paragraphs 
{
	width: 750px;
	margin: 0 auto;
	height: 300px
	background: red;
	margin-top: 60px;
	font-family: 'Droid Serif';
	font-size: 16px;
}
#title
{
	font-family: 'Droid Sans';
	font-size: 25px;
	width: 100%;
	height: 60px;
	padding-top: 10px;
	margin: 0 auto;
}
#contact
{
	width: 100%;
	height: 84px;
	background: url('footer.png');
	border-top: 2px solid white;
}
#info
{
	width: 800px;
	height: 50px;
	margin: 0 auto;

}

