body
{
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	background-size: cover !important;
	background-attachment: fixed;
}
/*--------FLAGS STARTS--------*/
#flags
{
	width: 100%;
	height: 3%;
}

#flags ul
{
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 100%;
	height: 100%;
}

#flags ul li
{
	display: inline;
	margin-left: 10px;
}

#flags img
{
	height: 100%;
}
/*--------FLAGS ENDS--------*/

/*--------NAVIGATION STARTS--------*/

#navigation
{
	width: 100%;
	height: 5%;
}

#navigation ul
{
	list-style-type: none;
	margin: 0 auto;
	padding: 0;
	text-align: center;
	width: 900px;
	height: 100%;
}

#navigation ul li
{
	display: inline-block;
	margin-left: 10px;
	color: white;
	margin-top: 10px;
	margin-left:30px;
}

#navigation a
{
	font-size: 25px;
	font-family: 'Droid Sans';
	font-weight: bold;
	text-decoration: none;
	color: black;
}
/*--------NAVIGATION ENDS--------*/

#top_fence
{
	width: 100%;
	height: 3%;
}
#heading
{
	width: 200px;
	height: 4%;
	font-family: 'Droid Sans';
	font-size: 26px;
	border-bottom: 2px solid gray;
}
#teenused_wrap
{
	width: 100%;
	min-height: 600px;
	height: auto;
	background: maroon;

}
.teenused
{
	min-height: 250px;
	width: 800px;
	margin: 0 auto;
	background: teal;
	list-style-type: none;
}
.teenus_box
{
	display: inline-block;
	width: 800px;
	margin-bottom: 50px;
}
.foto
{
	float: left;
	height: 200px;
	width: 200px;
	background: yellow;
}
.foto img
{
	height: 100%;
	width: 100%;
	overflow: hidden;
}
.seletus
{
	float: right;
	min-height: 200px;
	width: 580px;
}
/*---------SERVICES----------*/
.teenused table
{
	margin-bottom: 30px;
}
