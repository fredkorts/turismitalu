body
{
	height: 100%;
	width: 100%;
	margin: 0;
	padding: 0;
	background-size: cover !important;
	background-attachment: fixed;
}
/*--------FLAGS STARTS--------*/
#flags
{
	width: 100%;
	height: 3%;
}

#flags ul
{
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 100%;
	height: 100%;
}

#flags ul li
{
	display: inline;
	margin-left: 10px;
}

#flags img
{
	height: 100%;
}
/*--------FLAGS ENDS--------*/

/*--------NAVIGATION STARTS--------*/

#navigation
{
	width: 100%;
	height: 5%;
}

#navigation ul
{
	list-style-type: none;
	margin: 0 auto;
	padding: 0;
	text-align: center;
	width: 900px;
	height: 100%;
}

#navigation ul li
{
	display: inline-block;
	margin-left: 10px;
	color: white;
	margin-top: 10px;
	margin-left:30px;
}

#navigation a
{
	font-size: 25px;
	font-family: 'Droid Sans';
	font-weight: bold;
	text-decoration: none;
	color: black;
}
/*--------NAVIGATION ENDS--------*/

#top_fence
{
	width: 100%;
	height: 3%;
}
#heading
{
	width: 200px;
	height: 4%;
	font-family: 'Droid Sans';
	font-size: 26px;
	border-bottom: 2px solid gray;
}
#event_space
{
	height: auto;
	width: 100%;

}
#events
{
	width: 800px;
	height: 100%;
	margin: 0 auto;
}
#events ul 
{
	list-style-type: none;
	font-family: 'Sans Droid';
	font-size: 22px;
	font-weight: bold;
}
#events li
{
	display: inline-block;
}
.pildid
{
	display: inline-block;
	width: 90%;
	margin: 0 auto;
	min-height: 260px;
	height: auto;
}
.pildid img
{
	display: inline-block;
}
.event_box
{
	height: 120px;
	width: 300px;
	display: inline-block;
}
.event_title
{
	width: 100%;
	height: 30px;
	font-family: 'Droid sans';
	font-size: 23px;
}
.event_date
{
	width: 100%;
	height: 20px;
	text-align: left;
	font-family: 'Droid Sans';
	font-size: 17px;
}
.event_intro
{
	width: 100%;
	height: 70px;
	font-family: 'Droid Serif';
	font-size: 13px;
	font-weight: normal;
}
.event_jpg
{
	height: 120px;
	width: 120px;
	float: left;
}
.event_jpg img
{
	width: 100%;
	height: 100%;
}
.event_desc
{
	width: 175px;
	height: 120px;
	float: right;
}
.event_desc h1
{
	font-family: 'Droid Sans'
	font-size: 20px;
	font-weight: bold;
}
#contact
{
	width: 100%;
	height: 9%;
	background: gray;
	position: fixed;
	bottom: 0px;
	right: 0px;
}
#info
{
	width: 800px;
	height: 50px;
	margin: 0 auto;

}

